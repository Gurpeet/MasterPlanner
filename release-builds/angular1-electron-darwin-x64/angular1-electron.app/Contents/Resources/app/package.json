{
  "name": "angular1-electron",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "electron ./index.js",
    "dist": "build",
    "test_win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName='Planning Manager'",
    "test_mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "test_linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "author": "Gurpreet Singh",
  "license": "ISC",
  "devDependencies": {
    "electron": "^1.7.9",
    "electron-builder": "^19.46.9"
  },
  "dependencies": {
    "angular": "^1.6.6",
    "angular-route": "^1.6.6",
    "angular-ui-grid": "^4.0.11",
    "angular-ui-router": "^0.4.3",
    "bootstrap": "^3.3.7",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.4",
    "electron-reload": "^1.2.2"
  }
}
